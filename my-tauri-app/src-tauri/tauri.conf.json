{
  "tauri": {
    "bundle": {
      "identifier": "com.example.mytauriapp",
      "icon": [
        "icons/icon.png"
      ],
      "name": "My Tauri App",
      "version": "0.1.0"
    },
    "build": {
      "beforeBuildCommand": "",
      "beforeDevCommand": "",
      "devPath": "../dist",
      "distDir": "../dist"
    },
    "tauri": {
      "window": {
        "title": "My Tauri App",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true
      },
      "security": {
        "csp": "default-src 'self'; img-src *; script-src 'self'; style-src 'self' 'unsafe-inline';"
      }
    }
  }
}